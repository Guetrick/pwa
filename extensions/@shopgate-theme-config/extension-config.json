{
  "version": "1.0.0",
  "id": "@shopgate/theme-config",
  "components": [
    {
      "id": "theme-config",
      "type": "subscribers",
      "path": "frontend/index.js"
    }
  ],
  "configuration": {},
  "steps": [
    {
      "path": "extension/mockMapPrice.js",
      "hooks": [
        "shopgate.catalog.getProducts.v1:after",
        "shopgate.catalog.getHighlightProducts.v1:after"
      ],
      "input": [{"key": "products"}],
      "output": [{"key": "products"}]
    },
    {
      "path": "extension/mockNotFoundProduct.js",
      "hooks": [
        "shopgate.catalog.getProducts.v1:after",
        "shopgate.catalog.getHighlightProducts.v1:after"
      ],
      "input": [{"key": "products"}],
      "output": [{"key": "products"}]
    },
    {
      "path": "extension/mockMinMaxOrder.js",
      "hooks": [
        "shopgate.catalog.getProducts.v1:after",
        "shopgate.catalog.getHighlightProducts.v1:after"
      ],
      "input": [{"key": "products"}],
      "output": [{"key": "products"}]
    }
  ]
}
